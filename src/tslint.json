{
  "extends": "../tslint.json",
  "rules": {
    "angular-whitespace": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "trackBy-function": false,
    "no-output-named-after-standard-event": false
  }
}
