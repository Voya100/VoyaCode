{
  "name": "voyacode-json-package",
  "version": "1.0.0",
  "scripts": {
    "start": "set NODE_ENV=development && concurrently \"webpack --watch\" \"lite-server -c lite-config.json\"",
    "start-old": "tsc && concurrently \"tsc -w\" \"lite-server -c lite-config.json\" ",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server -c lite-config.json",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json --locale=en-US",
    "webpack-dev": "set NODE_ENV=development && webpack",
    "prod": "npm run ngc && set NODE_ENV=production && webpack --progress"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.7",
    "@angular/compiler": "~2.4.7",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "~2.4.3",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "underscore": "^1.8.3",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.7",
    "@ngtools/webpack": "^1.2.9",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^7.0.0",
    "angular-router-loader": "^0.5.0",
    "angular2-perfect-scrollbar": "^2.0.3",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.3.0",
    "performance-now": "^0.2.0",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "tslint": "^4.0.0",
    "typescript": "~2.0.10",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webdriver-manager": "10.2.5",
    "webpack-dev-server": "2.2.1"
  },
  "repository": {}
}
