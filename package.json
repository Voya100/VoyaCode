  "version": "1.0.0",
  "scripts": {
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "global-styles": "node-sass app/styles.scss app/styles.css",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "prod": "set NODE_ENV=production && webpack --progress",
    "start": "concurrently \"npm run webpack-dev\" \"npm run server\" ",
    "server": "cd server && npm start",
    "test": "karma start",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "webpack-dev": "set NODE_ENV=development && webpack --watch"
